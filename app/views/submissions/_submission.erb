<% form_for(submission, :url => { :action => 'delete'}) do |f| %>
	<div id='submission_<%=submission.id %>'>
		<%= f.hidden_field :id %>
		<%= f.submit 'x', :class => 'deleter'%>
		<em><%= submission.submission %></em>
		<strong><%= submission.name %> at <%= submission.created_at %></strong>
	</div>
<% end %>